<div class='nested_fields'>
  <%= f.inputs do %>
    <%= f.input :exercise %>
    <div class='sets'>
      <%= f.semantic_fields_for :sets do |set| %>
        <%= render 'set_fields', f: set %>
      <% end %>
      <div class='links'>
        <%= link_to_add_association 'add set', f, :sets %>
      </div>
    </div>
    <%= link_to_remove_association 'remove strength exercise', f %>
  <% end %>
</div>
